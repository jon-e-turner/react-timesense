{
  "expo": {
    "name": "react-timesince",
    "slug": "timesince",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "timesince",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.timesince"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#004a77",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "platforms": [/* "android", */ "ios", "web"],
    "githubUrl": "https://github.com/jon-e-turner/react-timesince",
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#004a77",
          "dark": {
            "backgroundColor": "#004a77"
          }
        }
      ],
      [
        "expo-sqlite",
        {
          "enableFTS": true
          // "useSqlCipher": true
        }
      ],
      [
        "expo-asset",
        {
          "assets": ["./assets/db/time-sense.db", "./assets/db/sqlite-init.sql"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
