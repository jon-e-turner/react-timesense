  "overrides": {
    "react": "19.1.0"
  },
  "dependencies": {
    "@date-fns/utc": "^2.1.1",
    "@expo/vector-icons": "^15.0.3",
    "@react-navigation/bottom-tabs": "^7.9.1",
    "@react-navigation/elements": "^2.9.3",
    "@react-navigation/native": "^7.1.27",
    "@testing-library/react-native": "^13.3.3",
    "date-fns": "^4.1.0",
    "expo": "~54.0.31",
    "expo-checkbox": "~5.0.8",
    "expo-constants": "~18.0.12",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-linking": "~8.0.11",
